{
    "source": {
      "include": [
        "mod"
      ],
      "includePattern": ".js$"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "opts": {
      "encoding": "utf8",
      "readme": "./README.md",
      "destination": "docs/xyz/",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/clean-jsdoc-theme"
    },
    "markdown": {
      "hardwrap": false,
      "idInHeadings": true
    }
  }