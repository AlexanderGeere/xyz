{
  "source": {
    "include": [
      "lib"
    ],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  
  "opts": {
    "encoding": "utf8",
    "readme": "./lib/README.md",
    "destination": "docs/mapp/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "include_js": [
        "./public/js/docs/navbar.js"
    ],
      "title": "Mapp",
      "homepageTitle": "Mapp",
      "favicon": "../public/icons/favicon.ico"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}